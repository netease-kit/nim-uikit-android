<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/cl_root"
    android:background="@color/color_e9eff5"
    tools:ignore="MissingDefaultResource">

    <com.netease.yunxin.kit.common.ui.widgets.BackTitleBar
        android:id="@+id/setting_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        app:titleText="@string/uikit_function_title"/>

    <LinearLayout
        android:id="@+id/kitSettingll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dimen_20_dp"
        android:layout_marginLeft="@dimen/dimen_20_dp"
        android:layout_marginTop="@dimen/dimen_16_dp"
        android:background="@drawable/app_white_round_bg"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@id/setting_title_bar">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView
                android:id="@+id/kitTeamTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_team_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitTeamSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:clickable="false"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:checked="true"
                android:thumb="@drawable/switch_thumb_selector"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView
                android:id="@+id/kitTeamModeTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_team_mode_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitTeamModeSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:clickable="false"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:checked="true"
                android:thumb="@drawable/switch_thumb_selector"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitCollectTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_collect_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitCollectSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitPinTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_pin_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitPinSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitStickTopTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_stick_top_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitStickTopSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitOnlineTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_online_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitOnlineSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitCallTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_call_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitCallSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_46_dp">

            <TextView android:id="@+id/kitImagePickTv"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_image_pick_switch_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/kitImagePickSc"
                android:layout_width="@dimen/dimen_60_dp"
                android:layout_height="@dimen/dimen_20_dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:thumb="@drawable/switch_thumb_selector"
                android:layout_marginEnd="@dimen/dimen_16_dp"
                android:clickable="false"
                android:checked="true"
                app:track="@drawable/switch_track_selector" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_1_dp"
            android:background="@color/color_f5f8fc" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_96_dp"
            android:orientation="vertical">

            <TextView android:id="@+id/kitTeamManager"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_46_dp"
                android:gravity="center"
                android:text="@string/uikit_team_manager_limit_title"
                android:textColor="@color/color_333333"
                android:layout_marginStart="@dimen/dimen_16_dp"
                android:textSize="@dimen/dimen_16_dp"
                tools:ignore="SpUsage" />

            <EditText
                android:id="@+id/kitTeamManageCountEt"
                android:layout_width="@dimen/dimen_96_dp"
                android:layout_height="40dp"
                android:background="@drawable/app_gray_round_bg"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:textColor="@color/color_333333"
                android:paddingEnd="35dp"
                android:singleLine="true"
                android:textSize="16dp"
                tools:ignore="SpUsage" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>